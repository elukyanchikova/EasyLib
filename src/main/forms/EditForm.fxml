<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Button GridPane.rowIndex = "0" GridPane.columnIndex = "0"  fx:id="addFileBtn" text="Add file" GridPane.halignment="CENTER"
              prefWidth="250" maxWidth="-Infinity" onMouseClicked="#addFileBtn" />

      <Button GridPane.rowIndex = "0"  GridPane.columnIndex = "1" fx:id="addUserBtn" text="Add User" GridPane.halignment="CENTER"
              prefWidth="250"  maxWidth="-Infinity" onMouseClicked="#addUserBtn"/>

      <Button GridPane.rowIndex = "1"  GridPane.columnIndex = "0"  fx:id="deleteFileBtn" text="Delete file" GridPane.halignment="CENTER"
              prefWidth="250" maxWidth="-Infinity" onMouseClicked="#deleteFileBtn" />

      <Button GridPane.rowIndex = "1"  GridPane.columnIndex = "1"  fx:id="deleteUerBtn" text="Delete User" GridPane.halignment="CENTER"
              prefWidth="250" maxWidth="-Infinity" onMouseClicked="#DeleteUserBtn" />

      <Button GridPane.rowIndex = "2" GridPane.columnIndex = "0"  fx:id="modifyFileBtn" text="Modify File" GridPane.halignment="CENTER"
              prefWidth="250" maxWidth="-Infinity" onMouseClicked="#modifyFileBtn"/>

      <Button GridPane.rowIndex = "2"  GridPane.columnIndex = "1"  fx:id="modifyUserBtn" text="Modify User" GridPane.halignment="CENTER"
              prefWidth="250" maxWidth="-Infinity" onMouseClicked="#modifyUserBtn" />
   </children>
</GridPane>
